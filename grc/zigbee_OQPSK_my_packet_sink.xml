<?xml version="1.0"?>
<block>
  <name>Packet sink</name>
  <key>zigbee_OQPSK_my_packet_sink</key>
  <category>[zigbee_OQPSK]</category>
  <import>import zigbee_OQPSK</import>
  <make>zigbee_OQPSK.my_packet_sink($threshold, $dsss_mode, $verbose, $verbose2)</make>

	<param>
		<name>Threshold</name>
		<key>threshold</key>
		<value>10</value>
		<type>int</type>
	</param>

  <param>
		<name>Dsss mode</name>
		<key>dsss_mode</key>
		<value>32</value>
		<type>int</type>
	</param>

  <param>
		<name>Verbose</name>
		<key>verbose</key>
		<value>0</value>
		<type>int</type>
	</param>

  <param>
		<name>Verbose2</name>
		<key>verbose2</key>
		<value>0</value>
		<type>int</type>
	</param>

	<sink>
		<name>in</name>
		<type>float</type>
		<nports>1</nports>
	</sink>
	<source>
		<name>out</name>
		<type>message</type>
	</source>
</block>
