<?xml version="1.0"?>
<block>
	<name>Packet Sink</name>
	<key>oqpsk_dsss_packet_sink</key>
	<category>[OQPSK_DSSS]</category>
	<import>import oqpsk_dsss</import>
	<make>oqpsk_dsss.packet_sink($threshold, $dsss_mode, $verbose, $verbose2)</make>

	<param>
		<name>Threshold</name>
		<key>threshold</key>
		<value>10</value>
		<type>int</type>
	</param>

  <param>
		<name>Dsss mode</name>
		<key>dsss_mode</key>
		<value>32</value>
		<type>int</type>
	</param>

  <param>
		<name>Verbose</name>
		<key>verbose</key>
		<value>0</value>
		<type>int</type>
	</param>

  <param>
		<name>Verbose2</name>
		<key>verbose2</key>
		<value>0</value>
		<type>int</type>
	</param>

	<sink>
		<name>in</name>
		<type>float</type>
		<nports>1</nports>
	</sink>
	<source>
		<name>out</name>
		<type>message</type>
	</source>
</block>
